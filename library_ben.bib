journal = {International Journal of Aeronautical and Space Sciences},
keywords = {Abrupt wing stall (AWS),Additional augmentation,FTR (free-to-roll),INDI (Incremental Nonlinear Dynamic Inversion),Uncommanded lateral motions,Wing drop,Wing heavy,Wing rock},
mendeley-groups = {Supersonic Aircraft/Various fighters},
number = {4},
