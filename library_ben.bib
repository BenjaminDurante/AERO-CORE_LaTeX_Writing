issn = {2226-4310},
journal = {Aerospace},
keywords = {Longitudinal dynamics,PIO,Pilot vehicle interaction,Stability analysis,Unmanned Aerial System},
mendeley-groups = {Handling Qualities},
month = {nov},
number = {4},
pages = {42},
title = {Analysis of Pilot-Induced-Oscillation and Pilot Vehicle System Stability Using {UAS} Flight Experiments},
