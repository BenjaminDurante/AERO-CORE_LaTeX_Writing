@inproceedings{A390Greene2014,
abstract = {The expanding capabilities of unmanned aircraft have resulted in a significant effort to accomplish missions previously performed by manned aircraft. In spite of this effort, insuffcient resources have been devoted to developing meaningful and appropriate ying qualities criteria and requirements for unmanned aircraft. The failure to employ ying qualities criteria and requirements tailored to unmanned aircraft has resulted in high costs for equipment (due to mishaps), manpower, and development time. This article addresses the need for establishing a ying qualities standard for unmanned aircraft, and proposes that a mission-based standard provides the most exibility as aircraft designs and missions continue to proliferate. A fundamental requirement for such a standard is a database of subjective and objective ying qualities assessments upon which requirements can be based. In addition, research is required to answer questions related to appropriate mission task elements, ying qualities parameters, and levels of autonomy.},
address = {Atlanta, Georgia},
annote = {A390 - 2014 - Toward a Flying Qualities Standard for Unmanned Aircraft},
